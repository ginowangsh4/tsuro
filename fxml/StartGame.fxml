<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>


<AnchorPane prefHeight="480.0" prefWidth="560.0" xmlns="http://javafx.com/javafx/9.0.4" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tsuro.admin.StartGameController">
   <children>
      <VBox alignment="CENTER" prefHeight="480.0" prefWidth="560.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome to our Tsuro Game! Click &quot;Start Game&quot; to start the game!" textAlignment="CENTER" textOrigin="CENTER">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </Text>
            <Button fx:id="startGameButton" mnemonicParsing="false" text="Start Game" />
         </children>
      </VBox>
   </children>
</AnchorPane>
